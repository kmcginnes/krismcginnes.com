{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "cSpell.words": [
    "fontawesome",
    "Fonticons",
    "frontmatter",
    "FSMB",
    "Influxer",
    "kmcginnes",
    "krismcginnes",
    "McGinnes",
    "Merch",
    "nextjs",
    "nunito",
    "Silverlight",
    "tailwindcss",
    "typecheck"
  ]
}
