{
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit",
    "source.fixAll.biome": "explicit"
  },
  "cSpell.words": [
    "fontawesome",
    "Fonticons",
    "frontmatter",
    "FSMB",
    "Influxer",
    "kmcginnes",
    "krismcginnes",
    "McGinnes",
    "Merch",
    "nextjs",
    "nunito",
    "Silverlight",
    "tailwindcss",
    "typecheck"
  ]
}
